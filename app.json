{
  "expo": {
    "name": "Bilmecelerce",
    "slug": "bilmece-dunyasi",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "bilmecelerce",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#A8D8EA"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bilmecedunyasi.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "Kisisellestirilmis reklamlar gostermek icin izninizi istiyoruz."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#A8D8EA"
      },
      "package": "com.bilmecedunyasi.app",
      "versionCode": 1,
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "4c4cf7c8-11d0-41e5-91da-57070f2ecdc6"
      },
      "router": {}
    },
    "owner": "ogishere"
  }
}
